#ifndef LIBCXX_NEW_HPP
#define LIBCXX_NEW_HPP

namespace liba9n::std
{
    void *operator new(size_t size, void *buffer) throw()
    {
        return buffer;
    }

    void *operator new[](size_t size, void *buffer) throw()
    {
        return buffer;
    }

    void operator delete(void *object) throw()
    {
        // delete object.
    }

    void operator delete[](void *object, void *buffer) throw()
    {
    }
}

#endif
